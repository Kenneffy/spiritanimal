<div id="signs">
	<form action="/form" method="POST">
		<select name="western">
			{{#each western}}
			<option value="{{id}}">{{name}}</option>
			{{/each}}

		</select>
	<br>
		<select name="eastern">
			{{#each eastern}}
			<option value="{{id}}">{{name}}</option>
			{{/each}}
		</select>
	<input type="submit" value="submit">
	</form>
</div>
