<h2>Western Sign</h2>
	<form action="/form" method="POST">
		<div>
			<select name="western" class="western">
				{{#each western}}
					<option value="{{id}}">{{name}}</option>
				{{/each}}
			</select>
			</div>
<h2>Eastern Sign</h2>
		<div>
			<select name="eastern" class="eastern">
				{{#each eastern}}
					<option value="{{id}}">{{name}}</option>
				{{/each}}
			</select>
		</div>
	<input type="submit" value="submit" id="submit">
	</form>


